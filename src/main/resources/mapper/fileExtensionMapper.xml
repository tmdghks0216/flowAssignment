<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.flow.fileExtension.repository.FileExtensionMapper">

    <resultMap id="fileExtensionResult" type="com.example.flow.fileExtension.entity.FileExtension"/>

    <select id="selectFileExtensionList" resultMap="fileExtensionResult">
        SELECT *
        FROM tb_file_filter
    </select>

    <update id="updateFixExtension">
        UPDATE tb_file_filter
        SET enable = #{enable},
            updatedt = NOW()
        WHERE extension = #{extension}
          AND type = 'FIX'
    </update>
</mapper>